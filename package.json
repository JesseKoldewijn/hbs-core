{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rimraf ./public/core ./public/styles && dotenv -- concurrently \"vite\" \"pnpm run compile:css:watch\" \"pnpm run compile:js:watch\"",
    "build": "rimraf ./.out && node ./build-project.cjs",
    "start": "dotenv -- serve ./out",
    "compile:site": "dotenv -- node ./generate-site.cjs",
    "compile:js:core": "node ./compile-assets.js && node ./minify-core.mjs --dev",
    "compile:js:core-prod": "node ./compile-assets.js && node ./minify-core.mjs --build",
    "compile:js:watch": "watch -d -p='/node_modules out/i public/i' 'pnpm run compile:js:core' ./src",
    "compile:css": "postcss ./src/styles/core.scss -o ./public/styles/core.css",
    "compile:css:watch": "watch -d -p='/node_modules out/i public/i' 'pnpm run compile:css' ./src",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@braintree/browser-detection": "^2.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.4",
    "@fastify/view": "^9.1.0",
    "@fastify/vite": "^6.0.7",
    "@minify-html/node": "^0.15.0",
    "@t3-oss/env-core": "^0.11.0",
    "chrono-node": "^2.7.6",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.10.0",
    "html-format": "^1.1.7",
    "htmx.org": "^2.0.1",
    "jquery": "4.0.0-beta.2",
    "moment": "^2.30.1",
    "watch": "^1.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@tailwindcss/postcss": "4.0.0-alpha.18",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.14.12",
    "@types/watch": "^1.0.6",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.23.0",
    "glob": "^11.0.0",
    "minify": "^11.4.0",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "postcss-nesting": "^12.1.5",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "serve": "^14.2.3",
    "tailwindcss": "4.0.0-alpha.18",
    "try-to-catch": "^3.0.1",
    "typescript": "^5.5.4",
    "vercel": "^35.2.1",
    "vite": "^5.3.5",
    "vite-plugin-node": "^3.1.0"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e",
  "volta": {
