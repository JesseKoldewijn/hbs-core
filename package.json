{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rimraf ./public/core ./public/styles && dotenv -- concurrently \"vite\" \"pnpm run compile:css:watch\" \"pnpm run compile:js:watch\"",
    "build": "rimraf ./.out && node ./build-project.cjs",
    "start": "dotenv -- serve ./out",
    "compile:site": "dotenv -- node ./generate-site.cjs",
    "compile:js:core": "node ./compile-assets.js && node ./minify-core.mjs --dev",
    "compile:js:core-prod": "node ./compile-assets.js && node ./minify-core.mjs --build",
    "compile:js:watch": "watch -d -p='/node_modules out/i public/i' 'pnpm run compile:js:core' ./src",
    "compile:css": "postcss ./src/styles/core.scss -o ./public/styles/core.css",
    "compile:css:watch": "watch -d -p='/node_modules out/i public/i' 'pnpm run compile:css' ./src",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@braintree/browser-detection": "^2.0.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.3",
    "@fastify/view": "^9.1.0",
    "@fastify/vite": "^6.0.6",
    "@minify-html/node": "^0.15.0",
    "@t3-oss/env-core": "^0.10.1",
    "chrono-node": "^2.7.5",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.9.0",
    "html-format": "^1.1.6",
    "htmx.org": "^1.9.12",
    "jquery": "4.0.0-beta",
    "moment": "^2.30.1",
    "watch": "^1.0.2",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@tailwindcss/postcss": "4.0.0-alpha.14",
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.14.10",
    "@types/watch": "^1.0.6",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.4.1",
    "esbuild": "^0.20.2",
    "glob": "^10.3.12",
    "minify": "^11.1.1",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-nesting": "^12.1.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.5",
    "sass": "^1.76.0",
    "serve": "^14.2.3",
    "tailwindcss": "4.0.0-alpha.14",
    "try-to-catch": "^3.0.1",
    "typescript": "^5.4.5",
    "vercel": "^34.1.8",
    "vite": "^5.2.11",
    "vite-plugin-node": "^3.1.0"
  }
}
