{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build",
  "outputDirectory": "out",
  "rewrites": [
    {
      "source": "/(.+)/$",
      "destination": "/$1",
      "statusCode": 308
    },
    {
      "source": "/(.*)\\.([^/]+)$",
      "destination": "/$1/index.$2",
      "statusCode": 200
    },
    {
      "source": "/\\.well-known/(.*)",
      "destination": "/.well-known/$1"
    }
  ],
  "headers": [
    {
      "source": "/\\.woff2$|\\.woff$|\\.ttf$|\\.html$|\\.js$|\\.css$|\\/.+\\.(woff2|woff|ttf|html|js|css)$/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=604800, stale-while-revalidate=59, immutable"
        }
      ]
    }
  ]
}
